{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "gGLieQGwKh8Nqxt6pQOxGvbLaJKzPQ8H7vduxKpMhEGSqDlh",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    },
    {
      "name": "ml_worker",
      "password_hash": "gGLieQGwKh8Nqxt6pQOxGvbLaJKzPQ8H7vduxKpMhEGSqDlh",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    },
    {
      "name": "services",
      "password_hash": "gGLieQGwKh8Nqxt6pQOxGvbLaJKzPQ8H7vduxKpMhEGSqDlh",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    }
  ],
  "vhosts": [
    {
      "name": "/",
      "description": "Default virtual host",
      "tags": []
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "ml_worker",
      "vhost": "/",
      "configure": "^ml_.*",
      "write": "^ml_.*",
      "read": "^(ml_.*|wwtp_.*)"
    },
    {
      "user": "services",
      "vhost": "/",
      "configure": "^(wwtp_.*|notification_.*)",
      "write": "^(wwtp_.*|notification_.*)",
      "read": "^(wwtp_.*|notification_.*)"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "wwtp_cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": "^(wwtp_|ml_|notification_)",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "ha-sync-batch-size": 1
      },
      "priority": 0
    },
    {
      "vhost": "/",
      "name": "message-ttl",
      "pattern": "^(wwtp_|ml_|notification_)",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 3600000,
        "max-length": 10000,
        "overflow": "reject-publish"
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "image_processing",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-priority": 10,
        "x-max-length": 1000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "notification_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-priority": 5,
        "x-max-length": 500
      }
    },
    {
      "name": "dead_letter_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000
      }
    }
  ],
  "exchanges": [
    {
      "name": "wwtp_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "notification_exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "dead_letter_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "wwtp_exchange",
      "vhost": "/",
      "destination": "image_processing",
      "destination_type": "queue",
      "routing_key": "image.analyze",
      "arguments": {}
    },
    {
      "source": "notification_exchange",
      "vhost": "/",
      "destination": "notification_queue",
      "destination_type": "queue",
      "routing_key": "notification.*",
      "arguments": {}
    },
    {
      "source": "dead_letter_exchange",
      "vhost": "/",
      "destination": "dead_letter_queue",
      "destination_type": "queue",
      "routing_key": "dead_letter",
      "arguments": {}
    }
  ]
}